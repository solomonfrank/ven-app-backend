{
  "extends": "airbnb-base",
  "plugins": ["prettier"],

  "rules": {
    // "prettier/prettier": "error",
    "import/prefer-default-export": 0,
    "no-console": "warn",
    "one-var-declaration-per-line": 0,
    "new-cap": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "import/named": 0,
    "arrow-parens": 0,
    "implicit-arrow-linebreak": 0,
    "function-paren-newline": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "req|res|val|next" }],
    "prefer-destructuring": [
      "error",
      {
        "array": true,
        "object": true
      },
      {
        "enforceForRenamedProperties": false
      }
    ]
  }
}

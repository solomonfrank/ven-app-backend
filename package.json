{
  "name": "ven-app-back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.13.1",
    "npm": "6.12.1"
  },
  "scripts": {
    "start": "node dist/",
    "start:dev": "nodemon --exec babel-node ./src/index.js",
    "start:prod": "cross-env NODE_ENV=production nodemon --exec babel-node ./src/index.js",
    "build": "rm -rf dist/ && babel src/ -d dist/",
    "migration:rollback": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "migration:rollback:prod": "cross-env NODE_ENV=production db:migrate:undo:all && npm run migration",
    "migration": "./node_modules/.bin/sequelize db:migrate",
    "migration:prod": "cross-env NODE_ENV=production ./node_modules/.bin/sequelize db:migrate",
    "heroku-postbuild": "npm install && npm run migration:rollback:prod && cross-env NODE_ENV=production && npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/register": "^7.10.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.11"
  }
}
